   "source": [
    "def insertionSort(arr):\n",
    "\tn = len(arr) \n",
    "\t\n",
    "\tif n <= 1:\n",
    "\t\treturn \n",
    "        \n",
    "\tfor i in range(1, n): \n",
    "\t\tkey = arr[i] \n",
    "\t\tj = i-1\n",
    "\t\twhile j >= 0 and key < arr[j]: \n",
    "\t\t\tarr[j+1] = arr[j] \n",
    "\t\t\tj -= 1\n",
    "\t\tarr[j+1] = key \n",
    "\n",
    "arr = [12, 11, 13, 5, 6]\n",
    "insertionSort(arr)\n",
    "print(arr)\n"
   ]
